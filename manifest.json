{
    "name": "Site Blocker",
    "description":"Blocks selected sites",
    "version":"1.0",
    "manifest_version": 2,
    "options_page":"options.html",
    "background":{
        "scripts":["background.js"],
        "persistent":false
     },
     "browser_action":{
        "default_popup":"options.html"
     },
     "permissions":["tabs","storage"]

}